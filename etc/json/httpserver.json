{
    "name": "httpserver",
    "label": "fgi",
    "listen": "/run/httpserver.sock",
    "owner": "nginx",
    "group": "nginx",
    "routes": [
        {
            "path": "/api/v1/auth",
            "service": "tokenauth.main"
        },{
            "path": "/api/v1/nginx",
            "service": "nginx.main"
        }
    ],
    "middleware": [ "log", "tokenauth.main" ]
}
